{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "create-block/child-block",
  "version": "0.1.0",
  "title": "Child Block",
  "category": "widgets",
  "icon": "smiley",
  "description": "Example block scaffolded with Create Block tool.",
  "example": {},
  "supports": {
    "html": false
  },
  "attributes": {
    "name": {
      "type": "string",
      "source": "html",
      "selector": "h4"
    },
    "bio": {
      "type": "string",
      "source": "html",
      "selector": "p"
    },
    "id": {
      "type": "number"
    },
    "alt": {
      "type": "string",
      "source": "attribute",
      "selector": "img",
      "attribute": "alt",
      "default": ""
    },
    "url": {
      "type": "string",
      "source": "attribute",
      "selector": "img",
      "attribute": "src"
    },
    "sizeSlug": {
      "type": "string",
      "default": "full"
    },
    "socialLinks": {
      "type": "array",
      "default": [
        {
          "link": "https://www.facebook.com/",
          "icon": "facebook"
        },
        {
          "link": "https://www.instagram.com/",
          "icon": "instagram"
        }
      ]
    }
  },
  "parent": [
    "create-block/faq-accordion"
  ],
  "textdomain": "child-block",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": "file:./view.js"
}